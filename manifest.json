{
    "name": "Json pretty",
    "version": "1.6",
    "description": "Extension by Kar-Utils from Karthikeyan Thiyagarajan",
    "permissions": ["activeTab","contextMenus","declarativeContent","management","storage"],
    "optional_permissions": [
      "http://*/","https://*/*"
    ],
    "options_page": "options.html",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Pretty Json",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },     
    "manifest_version": 2
  }